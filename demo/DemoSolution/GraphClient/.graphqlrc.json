{
	"schema": "schema.graphql",
	"documents": "**/*.graphql",
	"extensions": {
		"strawberryShake": {
			"name": "DemoClient",
			"namespace": "Demo.GraphQL",
			"url": "https://localhost:7060/graphql/",
			"records": {
				"inputs": false,
				"entities": false
			},
			"transportProfiles": [
				{
					"default": "Http",
					"subscription": "WebSocket"
				}
			]
		}
	}
}